@model MVCandSQLCONNECTION.Models.OswalDetails
@using System.Data;
@{
    ViewBag.Title = "Edit";
}


@using (Html.BeginForm("Edit", "Oswal", FormMethod.Post))
{
    @Html.HiddenFor(x => x.OswalId)
    <br />
    @Html.LabelFor(x => x.ClassId)
    @Html.DropDownListFor(x => x.ClassId,Model.OSlist)
    <br />
    @Html.LabelFor(x => x.StudentsId)
    @Html.DropDownListFor(x => x.StudentsId,Model.Stdlist)
    <br />
    <input type="submit"value="submit" />
}
